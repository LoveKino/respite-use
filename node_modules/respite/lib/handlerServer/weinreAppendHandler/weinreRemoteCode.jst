!(function(e) {
	var ipAddress = "{{ip}}";
	var port = "{{port}}";
	console && console.log("debug with " + ipAddress + ":" + port);
	e.setAttribute("src", "http://" + ipAddress + ":" + port +
		"/target/target-script-min.js#anonymous");
	document.getElementsByTagName("head")[0].appendChild(e);
})(document.createElement("script"));